<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequence for Print Jobs -->
        <record id="seq_seisei_print_job" model="ir.sequence">
            <field name="name">Seisei Print Job</field>
            <field name="code">seisei.print.job</field>
            <field name="prefix">PJ</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Printer Info -->
        <record id="seq_seisei_printer" model="ir.sequence">
            <field name="name">Seisei Printer Info</field>
            <field name="code">seisei.printer</field>
            <field name="prefix">PR</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Report Mapping -->
        <record id="seq_seisei_report_mapping" model="ir.sequence">
            <field name="name">Seisei Report Mapping</field>
            <field name="code">seisei.report.printer.mapping</field>
            <field name="prefix">RM</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>