<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="ir_cron_wechat_token_refresh" model="ir.cron">
        <field name="name">WeChat Work: Token Refresh</field>
        <field name="model_id" ref="seisei_messaging.model_seisei_messaging_channel"/>
        <field name="state">code</field>
        <field name="code">env['seisei.messaging.adapter.wechat_work']._cron_refresh_tokens()</field>
        <field name="interval_number">2</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
