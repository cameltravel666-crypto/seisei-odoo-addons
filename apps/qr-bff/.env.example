# QR-BFF Service Configuration

# Server
PORT=3100

# Redis (for token storage)
REDIS_HOST=odoo-redis
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_DB=1

# Odoo Connection
ODOO_URL=http://seisei-odoo-router:80
ODOO_SERVICE_LOGIN=qr_service
ODOO_SERVICE_PASSWORD=your_service_password
ODOO_TIMEOUT=10000
ODOO_RETRIES=1

# Default database (fallback when token not found in Redis)
DEFAULT_DB=ten_testodoo
