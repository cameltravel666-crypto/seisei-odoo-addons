# GitHub Container Registry
GITHUB_REPO_OWNER=cameltravel666-crypto
OCR_IMAGE_TAG=latest

# Gemini API
GEMINI_API_KEY=your-gemini-api-key

# OCR Database
OCR_DB_PASSWORD=your-ocr-db-password

